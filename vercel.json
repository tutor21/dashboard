{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "public/api/embed.html"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/embed.html",
      "dest": "/api"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ]
}